          <asp:Panel runat="server" ID="hintPopupHolder" CssClass="helpfulHint" Style="display: none;" Visible="false"><a href="#" id="helpfulHintCloseButton" class="closeButton">Dismiss</a><asp:Label runat="server" ID="hintText"></asp:Label></asp:Panel>
