            <asp:MenuItem Text="Views" Value="Views" NavigateUrl="../#"></asp:MenuItem>
            <asp:MenuItem Text="Aircraft" Value="Aircraft" NavigateUrl="/Aircraft_Listing.aspx"></asp:MenuItem>
