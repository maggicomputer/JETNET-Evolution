    <!-- End Container -->
    <!-- End Document -->
    <div style="display: none;">
      <asp:Button ID="status_notification_button" runat="server" OnClick="RemoveStatusNotification" />
    </div>

    <script type="text/javascript">
    // we run in the footer so no need to use onload -->
    // $(document).ready(function() {
    //  if ("<%= Me.WelcomeUser1.evo_message_text.text %>" != "") {
      //       $("#message").fadeIn("slow");
      //   }
      //});

      // function closeNotice() {
      //    $("#message").fadeOut("slow");
      // }



      $("#helpfulHintCloseButton").click(function () {
        $("#<%= hintUpdateButton.ClientID%>").click();
        $(".helpfulHint").slideUp("slow");
      });

      $(".helpfulHintLink").click(function () {
        $("#<%= hintUpdateClickLink.ClientID%>").click();
        $(".helpfulHint").slideUp("slow");
      });


      $(document).ready(function () {

        if ($('#<%= hintPopupHolder.ClientID %>').length) {
          $('#<%= hintPopupHolder.ClientID %>').slideDown("slow");
        }

        var menuTable = document.getElementById('<%= Menu1.ClientID %>');
        if ((typeof menuTable !== 'undefined') && (menuTable !== null)) {
          var menuLinks = menuTable.getElementsByTagName('a');
          for (i = 0; i < menuLinks.length; i++) {
            menuLinks[i].onclick = function () { ChangeTheMouseCursorOnItemParentDocument('cursor_wait'); }
          }
          setOnClickForNextLevelMenuItems(menuTable.nextSibling);
        }
      });


      function setOnClickForNextLevelMenuItems(currentMenuItemsContainer) {
        var id = currentMenuItemsContainer.id;
        if ((typeof id !== 'undefined') && (id !== null)) {
          var len = id.length;
          if (id != null && typeof (id) != 'undefined' && id.substring(0, parseInt(len) - 7) == '<%= Menu1.ClientID %>' && id.substring(parseInt(len) - 5, parseInt(len)) == 'Items') {
            var subMenuLinks = currentMenuItemsContainer.getElementsByTagName('a');
            for (i = 0; i < subMenuLinks.length; i++) {
              if (subMenuLinks[i].innerText == 'Values') {
              } else {
                subMenuLinks[i].onclick = function () { ChangeTheMouseCursorOnItemParentDocument('cursor_wait'); }
              }
            }
            setOnClickForNextLevelMenuItems(currentMenuItemsContainer.nextSibling);
          }
        }
      }
    </script>
