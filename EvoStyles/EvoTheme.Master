              <asp:MenuItem Text="Industry News" Value="Industry News" NavigateUrl="/evonews.aspx" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="JETNET Calendar" Value="JETNET Calendar" NavigateUrl="/help.aspx?t=4" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="Help" Value="Help" NavigateUrl="/help.aspx?t=2" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="JETNET News" Value="JETNET News" NavigateUrl="/help.aspx?t=3" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="Master Lists" Value="Master Lists" NavigateUrl="/MasterLists.aspx" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="Model Resources" Value="Model Resources" NavigateUrl="/help.aspx?t=5" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="Aircraft Glossary" Value="Aircraft Glossary" NavigateUrl="/help.aspx?t=6" Target="_blank"></asp:MenuItem>
              <asp:MenuItem Text="Yacht Glossary" Value="Yacht Glossary" NavigateUrl="/help.aspx?t=7" Target="_blank"></asp:MenuItem>
            </asp:MenuItem>
