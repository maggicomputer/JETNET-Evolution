      <asp:UpdatePanel runat="server" ID="hintUpdatePanel">
        <ContentTemplate>
          <asp:Button runat="server" ID="hintUpdateButton" Text="hintUpdateButtonClick" CssClass="display_none" />
          <asp:Button runat="server" ID="hintUpdateClickLink" Text="hintUpdateLinkClickEvent" CssClass="display_none" />
          <asp:Label runat="server" ID="hintTextUpdate" CssClass="display_none"></asp:Label>
        </ContentTemplate>
      </asp:UpdatePanel>
      <div class="sixteen columns footer">
        Copyright &copy; 2000 -
      <%= Now().Year.ToString()%>
