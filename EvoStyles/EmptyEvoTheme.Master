    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.10.20/af-2.3.4/b-1.6.1/b-colvis-1.6.1/b-html5-1.6.1/b-print-1.6.1/cr-1.5.2/fc-3.3.0/fh-3.1.6/kt-2.5.1/r-2.2.3/rg-1.1.1/rr-1.2.6/sc-2.0.1/sl-1.3.1/datatables.min.js"></script>
    <script type="text/javascript">

      if (Sys.Extended != undefined) {
        if (Sys.Extended.UI.TabContainer != undefined) {
          Sys.Extended.UI.TabContainer.prototype._app_onload = function (sender, e) {
            if (this._cachedActiveTabIndex != -1) {
              this.set_activeTabIndex(this._cachedActiveTabIndex);
              this._cachedActiveTabIndex = -1;

              var activeTab = this.get_tabs()[this._activeTabIndex];
              if (activeTab) {
                activeTab._wasLoaded = true;
                //activeTab._setFocus(activeTab); -- disable focus on active tab in the last TabContainer
              }
            }
            this._loaded = true;
          }
        }
      }
    </script>

    <div class="FixedHeaderBar" runat="server" id="fixedBar">
    </div>
    <asp:Literal ID="background_image" runat="server">  
        <img src="../images/background/11.jpg" alt="" class="bg_image" style="display:none;" />
    </asp:Literal>
    <!-- Primary Page Layout ================================================== -->
    <div class="container" runat="server" id="pageSizing">
      <!--Start Container-->
      <div id="container_white_background_div" runat="server" class="sixteen columns white_background_color content_border">
        <div id="container_border" runat="server">
          <div class="twelve columns headerHeight" id="WelcomeContainer" runat="server">
            <div class="six columns">
              <asp:Image ID="logo" ImageUrl="../../images/spacer.gif" runat="server" CssClass="evolution_logo_standalone" />
            </div>
            <div class="six columns specialHeaderEmpty">
              <asp:Literal runat="server" ID="extra_buttons" Visible="false"></asp:Literal>
              <asp:Literal runat="server" ID="updateHelp" Visible="false"></asp:Literal>
              <a href="javascript:window.close();" class="fa fa-home float_right white_text padding_right"
                id="homeButton" runat="server" visible="false"></a>
              <asp:Label ID="PageText" runat="server" CssClass="logo_text_title"
                ForeColor="Black">Page Title</asp:Label>
            </div>
          </div>
          <div id="belowWelcomeContainer" runat="server" class="headerHeightPadding">
          </div>

          <div class="contentContainer twelve columns remove_margin">
            <asp:Panel runat="server" ID="hintPopupHolder" CssClass="helpfulHint" Visible="false"><a href="#" id="helpfulHintCloseButton" class="closeButton">Dismiss</a><asp:Label runat="server" ID="hintText"></asp:Label></asp:Panel>
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
          </div>
        </div>
      </div>
      <asp:UpdatePanel runat="server" ID="hintUpdatePanel">
        <ContentTemplate>
          <asp:Button runat="server" ID="hintUpdateButton" Text="hintUpdateButtonClick" CssClass="display_none" />
          <asp:Button runat="server" ID="hintUpdateClickLink" Text="hintUpdateLinkClickEvent" CssClass="display_none" />
          <asp:Label runat="server" ID="hintTextUpdate" CssClass="display_none"></asp:Label>
        </ContentTemplate>
      </asp:UpdatePanel>
      <div class="twelve columns footer displayNoneMobile" runat="server" id="footer">
        Copyright &copy; 2000 -
      <%= Now().Year.ToString()%>
      JETNET LLC All rights reserved
      </div>
      <div class="clear">
      </div>
    </div>
