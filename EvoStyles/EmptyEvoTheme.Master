    jQuery("#helpfulHintCloseButton").click(function () {
      jQuery("#<%= hintUpdateButton.ClientID%>").click();
          jQuery(".helpfulHint").slideUp("slow");
        });

    jQuery(".helpfulHintLink").click(function () {
      jQuery("#<%= hintUpdateClickLink.ClientID%>").click();
          jQuery(".helpfulHint").slideUp("slow");
        });

    jQuery(function () {
      if (jQuery('#<%= hintPopupHolder.ClientID %>').length) {
            jQuery('#<%= hintPopupHolder.ClientID %>').slideDown("slow");
          }
          CreateMobileMenu(); //on load

          // if Modernizr detects class "touch"
          if (jQuery('html').hasClass('touch')) {
            // for each element with class "make-tel-link"
            jQuery(".make-tel-link").each(function () {
              var jPhoneNumber = jQuery(this).text();
              // wrap phone with href="tel:" and then insert phone number
              jQuery(this).wrapInner('<a class="jPhoneLink" href=""></a>');
              jQuery('.jPhoneLink').attr('href', 'tel:' + jPhoneNumber);
            });
          }
        });

    jQuery(document).mouseup(function (e) {
      var menu = jQuery('.slicknav_menu');
      if (!menu.is(e.target) // The target of the click isn't the container.
        && menu.has(e.target).length === 0) // Nor a child element of the container
      {
        jQuery('.slicknav_btn').removeClass("slicknav_open");
        jQuery('.slicknav_btn').addClass("slicknav_collapsed");
        jQuery('.slicknav_nav').addClass("slicknav_hidden");
        jQuery('.slicknav_nav').removeAttr("style");
        jQuery('.slicknav_nav').attr("style", "display:none;");
      }
    });


  </script>
