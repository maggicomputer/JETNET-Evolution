    <script type="text/javascript">
      // we run in the footer so no need to use onload -->

      $(document).ready(function () {

        $(".lowerLevel .searchBox").hide();
        $(".lowerLevel .searchPopupButton").on("tap click", function (event) {
          $(".lowerLevel .searchBox").toggle();
        });

        $(".Closenav").click(function () {
          var strTest = $(".Closenav").text();
          if (strTest.indexOf('Open Navigation') != -1) {
            $(".Closenav").html('<i class="fa fa-caret-square-o-up" aria-hidden="true"></i>Close Navigation');
          } else {
            $(".Closenav").html('<i class="fa fa-caret-square-o-down" aria-hidden="true"></i>Open Navigation');
          };
          $("#book").slideToggle("slow", function () {
            // Animation complete.
          });
        });

        if ("<%= Me.WelcomeUser1.evo_message_text.text %>" != "") {
                $("#message").fadeIn("slow");
              }
            });

      function closeNotice() {
        $("#message").fadeOut("slow");
      }
