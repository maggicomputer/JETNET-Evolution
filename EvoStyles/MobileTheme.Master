  <script type="text/javascript">
    jQuery(function () {
      CreateMobileMenu(); //on load
    });

    function CreateMobileMenu() {
      var isView = false;

      if (jQuery('#<%= isView.clientID %>').val() != '') {
            isView = true;
          }

          //$(".DetailsBrowseTable").prepend("<a href='#back'>Back to Top</a>");
          $(".DetailsBrowseTable").prepend("<a href='/home.aspx'>Home</a>");
          if (isView) {
            $(".DetailsBrowseTable").prepend("<a href='#star'>Star Reports</a>");
            $(".DetailsBrowseTable").prepend("<a href='#sales'>Retail Sales</a>");
       <%= IIF( HttpContext.Current.Session.Item("localSubscription").crmAerodexFlag = false, "$("".DetailsBrowseTable"").prepend(""<a href='#forsale'>For Sale</a>"");$("".DetailsBrowseTable"").prepend(""<a href='#trends'>Trends</a>"");","") %>
              $(".DetailsBrowseTable").prepend("<a href='#op'>Operating Costs</a>");
              $(".DetailsBrowseTable").prepend("<a href='#perf'>Performance Specs</a>");
              $(".DetailsBrowseTable").prepend("<a href='#fleet'>Fleet</a>");
        <%= IIF( HttpContext.Current.Session.Item("localSubscription").crmAerodexFlag = false, "$("".DetailsBrowseTable"").prepend(""<a href='#market'>Market Status</a>"");","") %>
              $(".DetailsBrowseTable").prepend("<a href='#desc'>Description</a>");
            } else {
             <%= IIF( HttpContext.Current.Session.Item("localSubscription").crmAerodexFlag = false, "$("".DetailsBrowseTable"").prepend(""<a href='/view_template.aspx?ViewID=27&ViewName'>Values</a>"");","") %>
        $(".DetailsBrowseTable").prepend("<a href='/Aircraft_Listing.aspx?e=1'>Event Activity</a>");
        $(".DetailsBrowseTable").prepend("<a href='/Performance_Listing.aspx'>Performance Specs</a>");
        $(".DetailsBrowseTable").prepend("<a href='/Operating_Listing.aspx'>Operating Costs</a>");
        $(".DetailsBrowseTable").prepend("<a href='/view_template.aspx?ViewID=1&ViewName=Model%20Market%20Summary'>Model Market Summary</a>");
        $(".DetailsBrowseTable").prepend("<a href='/Airport_Listing.aspx'>Airports</a>");
        $(".DetailsBrowseTable").prepend("<a href='/Company_Listing.aspx'>Companies</a>");
        $(".DetailsBrowseTable").prepend("<a href='/Aircraft_Listing.aspx'>Aircraft</a>");
      }

      jQuery('.DetailsBrowseTable').each(function () {
        var list = jQuery(this), ul = jQuery(document.createElement('ul')).replaceWith(jQuery('#mobileMenu'));
        ul.empty();
        ul.addClass("flexnav display_none");

        //Setting up the main links.
        jQuery('a', this).each(function () {
          var target = jQuery(this).attr('target'),
            option = jQuery(document.createElement('li'))
              .appendTo(ul)
          var aaa = jQuery('<a/>')
            .text(jQuery(this).text() == '' ? jQuery(this).attr("title") : jQuery(this).text())
            .attr('href', jQuery(this).attr("href"))
            .attr('class', jQuery(this).attr("class"))
            .attr('onclick', jQuery(this).attr("onclick"))
            .appendTo(option);
        });

        ul.slicknav({
          closeOnClick: true
        });
      });
    }

    jQuery(function () {
      CreateMobileMenu(); //on load
    });

    jQuery(document).mouseup(function (e) {
      var menu = jQuery('.slicknav_menu');
      if (!menu.is(e.target) // The target of the click isn't the container.
        && menu.has(e.target).length === 0) // Nor a child element of the container
      {
        jQuery('.slicknav_btn').removeClass("slicknav_open");
        jQuery('.slicknav_btn').addClass("slicknav_collapsed");
        jQuery('.slicknav_nav').addClass("slicknav_hidden");
        jQuery('.slicknav_nav').removeAttr("style");
        jQuery('.slicknav_nav').attr("style", "display:none;");
      }
    });

  </script>

  <div id="mobileMenu">
  </div>
