<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="YachtTheme.master.vb"
  Inherits="crmWebClient.YachtTheme" %>

<%@ Register Src="~/controls/WelcomeUser.ascx" TagName="WelcomeUser" TagPrefix="uc3" %>
<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
